{
  "cleanUrls": true,
  "builds": [
    {
      "src": "/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app/main.py"
    }
  ],
  "functions": {
    "app/main.py": {
      "excludeFiles": [
        "tests/**",
        ".devcontainer.json",
        ".dockerignore",
        "conftest.py",
        "Dockerfile",
        "LICENSE"
      ]
    }
  }
}
